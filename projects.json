{
    "default_build": {
        "command": "make",
        "args": [
            "-s"
        ]
    },
    "default_clean": {
        "command": "make",
        "args": [
            "clean",
            "-s"
        ]
    },
    "projects": {
        "Bootloader": {
            "path": "./bootloader",
            "output_path": "target/x86_64-unknown-uefi/debug/bootloader.efi",
            "install_path": "EFI/BOOT/BOOTX64.EFI",
            "build_command": {
                "command": "cargo",
                "args": [
                    "build"
                ]
            },
            "clean_commad": {
                "command": "cargo",
                "args": [
                    "clean"
                ]
            }
        },
        "Kernel": {
            "path": "./kernel",
            "output_path": "target/x86_64-los/debug/kernel",
            "install_path": "kernel.elf",
            "build_command": {
                "command": "cargo",
                "args": [
                    "build"
                ]
            },
            "clean_commad": {
                "command": "cargo",
                "args": [
                    "clean"
                ]
            }
        },
        "lib-c": {
            "path": "./libraries/lib-c",
            "output_path": "libc.a",
            "install_path": "los/lib/libc.a",
            "build_command": {
                "command": "make",
                "args": [
                    "-s",
                    "install"
                ]
            }
        },
        "lib-kernel": {
            "path": "./libraries/lib-kernel",
            "output_path": "libkernel.a",
            "install_path": "los/lib/libkernel.a",
            "build_command": {
                "command": "make",
                "args": [
                    "-s",
                    "install"
                ]
            }
        },
        "Shell": {
            "path": "./shell",
            "output_path": "shell.app",
            "install_path": "los/shell.app"
        },
        "cat": {
            "path": "./programs/cat",
            "output_path": "cat.app",
            "install_path": "los/cat.app"
        },
        "printenv": {
            "path": "./programs/printenv",
            "output_path": "printenv.app",
            "install_path": "los/printenv.app"
        },
        "ls": {
            "path": "./programs/ls",
            "output_path": "ls.app",
            "install_path": "los/ls.app"
        },
        "pwd": {
            "path": "./programs/pwd",
            "output_path": "pwd.app",
            "install_path": "los/pwd.app"
        },
        "echo": {
            "path": "./programs/echo",
            "output_path": "echo.app",
            "install_path": "los/echo.app"
        }
    },
    "configurations": {
        "default": [
            "Bootloader",
            "Kernel",
            "lib-kernel",
            "lib-c",
            "Shell",
            "cat",
            "ls",
            "pwd",
            "echo",
            "printenv"
        ],
        "user": [
            "lib-kernel",
            "lib-c",
            "Shell",
            "cat",
            "ls",
            "pwd",
            "echo",
            "printenv"
        ]
    }
}