{
    "default_build": {
        "command": "cargo",
        "args": [
            "build"
        ]
    },
    "default_clean": {
        "command": "cargo",
        "args": [
            "clean"
        ]
    },
    "projects": {
        "Bootloader": {
            "path": "./bootloader",
            "output_path": "target/x86_64-unknown-uefi/debug/bootloader.efi",
            "install_path": "EFI/BOOT/BOOTX64.EFI"
        },
        "Kernel": {
            "path": "./kernel",
            "output_path": "target/x86_64-los/debug/kernel",
            "install_path": "kernel.elf"
        },
        "lib-c": {
            "path": "./libraries/lib-c",
            "output_path": "libc.a",
            "install_path": "los/lib/libc.a",
            "build_command": {
                "command": "make",
                "args": [
                    "install"
                ]
            },
            "clean_command": {
                "command": "make",
                "args": [
                    "clean"
                ]
            }
        },
        "lib-kernel": {
            "path": "./libraries/lib-kernel",
            "output_path": "libkernel.a",
            "install_path": "los/lib/libkernel.a",
            "build_command": {
                "command": "make",
                "args": [
                    "install"
                ]
            },
            "clean_command": {
                "command": "make",
                "args": [
                    "clean"
                ]
            }
        },
        "shell": {
            "path": "./shell",
            "output_path": "shell.app",
            "install_path": "los/shell.app",
            "build_command": {
                "command": "make"
            },
            "clean_command": {
                "command": "make",
                "args": [
                    "clean"
                ]
            }
        }
    },
    "configurations": {
        "default": [
            "Bootloader",
            "Kernel",
            "lib-c",
            "lib-kernel",
            "shell"
        ],
        "user": [
            "lib-c",
            "lib-kernel",
            "shell"
        ]
    }
}